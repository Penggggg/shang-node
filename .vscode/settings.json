// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/*.map": true,
        "**/*.module.js": true,
        "**/*.component.js": true,
        "**/*.interface.js": true,
        "**/index.js": true,
        "**/*.server.js": true,
        "**/*.mock.js": true,
        "**/*.directive.js": true
    }
}